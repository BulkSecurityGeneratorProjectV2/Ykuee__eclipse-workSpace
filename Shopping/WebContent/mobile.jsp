			var wait = 60;
			function time(o) {
				if (wait == 0) {
					o.removeAttribute("disabled");
					o.setAttribute('class', 'sms-but');
					o.value = "获取验证码";
					wait = 60;

				} else {
					o.setAttribute("disabled", true);
					o.setAttribute('class', 'sms-but-hover');
					o.value = "重新发送(" + wait + ")";
					wait--;
					setTimeout(function() {
						time(o);
					}, 1000)
				}
			}
			var result_sms = false;
			$('#sms-but')
					.click(
							function() {
								check_img_code();
								var obj = this;
								var name = $("#name").val();
								if (name.length == 0) {
									lineState("reginfo", "error", "请输入您的手机号码！");
									return;
								} else {
									lineState("reginfo", "corect", "");
									if (!is_username) {
										lineState("reginfo", "error",
												"您输入的手机号码不正确！");
										return;
									} else {
										lineState("reginfo", "corect", "");
									}
								}

								if (result_sms) {
									$
											.ajax({
												cache : true,
												type : "POST",
												dataType : "json",
												url : '/index.php/passport-send_vcode_sms.html',
												data : "uname="
														+ $("#name").val()
														+ "&type=mobile"
														+ "&signupverifycode="
														+ jQuery.trim(jQuery(
																'#smsCode')
																.val())
														+ "&is_ajax=1&sign_type=diglog",
												async : false,
												error : function(request) {
													alert("出错啦，请刷新重试！");
												},
												success : function(data) {
													if (data.success) {
														time(obj);
														//lineState("vcode","error","验证码已经发送！");
													} else {
														alert(data.error);
													}
												}
											});
								}
							})
			$('#license').click(function() {
				if ($("#license").get(0).checked) {
					lineState("regvcode", "none", "");
