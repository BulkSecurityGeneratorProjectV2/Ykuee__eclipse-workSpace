				//筛选单项更多
				$(document).on('click', '.n-screen .more', function() {
					screenLiMore(this);
				})
				$(document).on('click', '.n-screen .bmore', function() {
					screenBrandMore(this);
				})

				//筛选整体隐藏个数
				$('.n-screen li').eq(screenLiNub - 1).nextAll().hide();
				//$('.n-screen li:last').show();

				//筛选整体更多事件
				$(document).on('click', '.n-show-more', function() {
					screenAllMore(this);
				})

				//选择新品人气，打开筛选
				$('.sort .first').bind('click', function() {
					if ($('.n-screen').css('display') == "none") {
						$('.n-screen').stop(true, true).fadeIn(300);
					} else {
						$('.n-screen').hide();
					}
				})

			})

			//筛选整体更多
			function screenAllMore(obj) {
				var parentUl = $('#screen-ul');

				if ($(obj).attr('data-state') == "show") {
					parentUl.find('li').fadeIn(200);
					$(obj).html("-收起");
					$(obj).attr('data-state', 'hide');
				} else {
					$('li', parentUl).eq(screenLiNub - 1).nextAll().hide();
					$('#li-last', parentUl).show();
					$(obj).html("+更多查找条件");
					$(obj).attr('data-state', 'show');
				}
			}

			//筛选单个更多
			function screenLiMore(obj) {
				var nub = 34;
				var parentBox = $(obj).parent();
				if ($(obj).hasClass('more-hover')) {
					parentBox.css('height', nub);
					parentBox.prev().height(nub);
					$(obj).removeClass('more-hover');
				} else {
					parentBox.css('height', 'auto');
					parentBox.prev().height(parentBox.height());
					$(obj).addClass('more-hover');
				}
			}
			//筛选单个更多
			function screenBrandMore(obj) {
				var nub = 59;
				var parentBox = $(obj).parent();
				if ($(obj).hasClass('bmore-hover')) {
					parentBox.css('height', nub);
					parentBox.prev().height(nub);
					$(obj).removeClass('bmore-hover');
				} else {
					parentBox.css('height', 'auto');
					parentBox.prev().height(parentBox.height());
					$(obj).addClass('bmore-hover');
				}
			}
