				} else {
					lineState("regvcode", "error", "请同意会员注册协议和隐私保护政策！");
					return;
				}
			})
			$('#vcode').blur(function() {
				if (!$.trim($('#vcode').val())) {
					lineState("regvcode", "error", "请输入验证码！");
					is_vcode = 0;
					return;
				}
				$.ajax({
					cache : true,
					type : "POST",
					dataType : "html",
					url : '/index.php/passport-check_code.html',
					async : false,
					error : function(request) {
						alert("出错啦，请刷新重试！");
					},
					success : function(data) {
						if (data == $.trim($('#vcode').val())) {
							lineState("regvcode", "corect", "");
							is_vcode = 1;
							return;
						} else {
							is_vcode = 0;
							lineState("regvcode", "error", "验证码错误！");
							return;
						}
					}
				});
			})

			$('#smsCode').blur(function() {
				check_img_code();
			})

			function check_img_code() {
				if (!$.trim($('#smsCode').val())) {
					lineState("smsvcode", "error", "请输入图片验证码！");
					return;
				}
				$.ajax({
					cache : true,
					type : "POST",
					data : {
						signupverifycode : $.trim($('#smsCode').val())
					},
					dataType : "html",
					url : '/index.php/passport-check_img_code.html',
					async : false,
